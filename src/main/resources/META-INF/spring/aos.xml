<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.jahiacommunity.modules.aos.AOSFilter">
        <property name="description" value="Filter that animate on scroll the current resource."/>
        <property name="priority" value="46"/>
        <property name="applyOnNodeTypes" value="jmix:droppableContent"/>
        <property name="skipOnConfigurations" value="include,wrapper"/>
        <property name="skipOnModes" value="studio"/>
    </bean>
</beans>
